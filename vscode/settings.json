{
    "[shellscript]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "docker.showExplorer": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "block",
    "editor.fontFamily": "'monofur For Powerline', monofur, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.insertSpaces": true,
    "editor.minimap.enabled": false,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        80
    ],
    "editor.scrollBeyondLastLine": false,
    "editor.wordWrap": "on",
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        "Brewfile": "ruby",
        "APKBUILD": "shellscript",
        "Dockerfile.*": "dockerfile",
        "ssh_config": "ssh_config"
    },
    "files.autoSave": "onFocusChange",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.ignoreLimitWarning": true,
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.codeLens.scopes": [
        "document",
        "containers"
    ],
    "gitlens.currentLine.enabled": false,
    "gitlens.explorers.avatars": false,
    "gitlens.gitExplorer.enabled": false,
    "gitlens.gitExplorer.files.layout": "auto",
    "gitlens.hovers.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.recentChanges.highlight.locations": [
        "gutter",
        "line",
        "overview"
    ],
    "gitlens.statusBar.alignment": "left",
    "gitlens.statusBar.enabled": false,
    "go.gopath": "/Users/orax/Projects/Go",
    "go.useLanguageServer": false,
    "go.vetOnSave": "package",
    "markdown.extension.quickStyling": true,
    "markdown.extension.showExplorer": false,
    "markdown.preview.scrollEditorWithPreview": false,
    "shellcheck.run": "onSave",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.fontSize": 14,
    "todo-tree.expanded": true,
    "todo-tree.flat": false,
    "todo-tree.iconColour": "grey",
    "todo-tree.iconColours": {
        "BUG": "#bf616a",
        "CHANGED": "#C0C5CE",
        "COMBAK": "#d08770",
        "FIXME": "#bf616a",
        "HACK": "#d08770",
        "IDEA": "#C0C5CE",
        "NB": "#ebcb8b",
        "NOTE": "#ebcb8b",
        "QUESTION": "#a3be8c",
        "REVIEW": "#a3be8c",
        "TEMP": "#d08770",
        "TODO": "#8fa1b3",
        "XXX": "#C0C5CE"
    },
    "todo-tree.regex": "(//|#|<!--|;)\\s*(TODO|FIXME|CHANGED|XXX|IDEA|HACK|NOTE|REVIEW|NB|BUG|QUESTION|COMBAK|TEMP)",
    "todo-tree.ripgrep": "/usr/local/bin/rg",
    "[toml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2
    },
    "window.restoreWindows": "one",
    "window.zoomLevel": 0,
    "wordscounter.enableLanguageFilter": true,
    "wordscounter.languageFilter": [
        "markdown",
        "plaintext"
    ],
    "workbench.colorTheme": "Ocean Dark Extended",
    "workbench.iconTheme": "file-icons",
    "workbench.startupEditor": "none",
    "workbench.statusBar.feedback.visible": false,
    "zenMode.fullScreen": false,
    "[makefile]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": false
    },
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "todo-tree.showInExplorer": false,
    "gitlens.historyExplorer.enabled": true,
    "editor.fontLigatures": true,
}
