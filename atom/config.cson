        red: 242
        green: 119
        blue: 122
        alpha: 1
      yellow:
        red: 255
        green: 204
        blue: 102
        alpha: 1
    style: {}
    toggles:
      autoClose: true
  "tool-bar":
    iconSize: "16px"
    position: "Left"
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
  wordcount:
    extensions: [
      "md"
      "markdown"
      "readme"
      "txt"
      "rst"
      "adoc"
      "log"
      "msg"
      "tex"
      "latex"
    ]
    goalLineHeight: "0%"
".basic.html.text":
  editor:
    tabLength: 2
".coffee.source":
  editor:
    tabLength: 2
".cson.source":
  editor:
    tabLength: 2
".fish.source":
  editor:
    tabLength: 4
".json.source":
  editor:
    tabLength: 2
".ruby.source":
  editor:
    tabLength: 2
".source.toml":
  editor:
    tabLength: 2
".source.yaml":
  editor:
    tabLength: 2
